{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "moduleResolution": "Node",
    "allowJs": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "paths": {
      "~/*": ["./*"],
      "ol": ["node_modules/ol/src"],
      "ol/*": ["node_modules/ol/src/*"]
    },
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "typeRoots": []
  },
  "include": ["src/**/*"],
  "typeAcquisition": {
    "exclude": ["ol"]
  }
}
